  "license": "Integrify",
  "dependencies": {
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.1",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "mongoose": "^5.2.6",
    "react-modal": "^3.5.1"
  },
  "devDependencies": {
    "nodemon": "^1.18.3"
  }
}
